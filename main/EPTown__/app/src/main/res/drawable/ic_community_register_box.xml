<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!-- 사각형의 배경색 -->
    <solid android:color="#585858"/>

    <!-- 사각형의 모서리 둥글게 설정 -->
    <corners android:radius="5dp"/>

    <!-- 사각형의 내부 패딩 설정 (선택 사항) -->
    <padding
        android:left="10dp"
        android:top="10dp"
        android:right="10dp"
        android:bottom="10dp"/>
</shape>